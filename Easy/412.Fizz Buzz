class Solution {
public:
    vector<string> fizzBuzz(int n) {

        vector<string> array(n);

        for(int i=0;i<=n-1;i++){
            if((i+1)%3 == 0 && (i+1)%5==0)
            array[i]="FizzBuzz";
            else if((i+1)%3==0)
            array[i]="Fizz";
            else if((i+1)%5==0)
            array[i]="Buzz";
            else
            array[i]=to_string(i+1);
        }
        return array;
        
    }
};
